        "scope": {
          "type": "string"
        },
        "cache": {
          "type": "string"
        },
        "search": {
          "type": "string"
        },
        "limit": {
          "type": "integer"
        },
        "includeDeleted": {
          "type": "boolean"
        },
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "namespace": {
          "type": "string"
        },
        "tagSelector": {
          "type": "string"
        },
        "labelSelector": {
          "type": "string"
        },
        "fieldSelector": {
          "type": "string"
        },
        "health": {
          "type": "string"
        },
        "types": {
          "$ref": "#/$defs/Items"
