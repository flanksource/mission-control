    "ViewConfigUIPlugin": {
      "properties": {
        "configTab": {
          "$ref": "#/$defs/ResourceSelector",
          "description": "ConfigTab is a selector that determines which config pages should display this view as a tab.\n\nWhen a config matches this selector, the view will appear as a tab on that config's detail page."
        },
        "variables": {
          "additionalProperties": {
            "type": "string"
          },
          "type": "object",
          "description": "Variables define template expressions to extract data from the config and pass them as variables to the view.\n\nThe map key is the variable name, and the value is a Go template expression that extracts data from the config.\nTemplates have access to the config object via the `.config` variable.\n\nExample:\n  variables:\n    namespace: \"$(.config.tags.namespace)\"\n    cluster: \"$(.config.tags.cluster)\""
        }
      },
      "additionalProperties": false,
      "type": "object",
      "required": [
        "configTab"
      ]
    },
    "ViewDisplay": {
