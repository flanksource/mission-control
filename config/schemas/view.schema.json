    "ViewCache": {
      "properties": {
        "maxAge": {
          "type": "string",
          "description": "MaxAge is the maximum age of a cache before it's deemed stale.\nCan be overridden with cache-control headers.\nDefault: 15m"
        },
        "minAge": {
          "type": "string",
          "description": "MinAge is the minimum age of a cache a user can request.\nDefault: 10s"
        },
        "refreshTimeout": {
          "type": "string",
          "description": "RefreshTimeout is the duration to wait for a view to process before returning stale data.\nDefault: 5s"
        }
      },
